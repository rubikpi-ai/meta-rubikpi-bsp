From 985587e14d80522f50e52d877e10475f517bd69f Mon Sep 17 00:00:00 2001
From: Linux Build Service Account <lnxbuild@localhost>
Date: Sun, 29 Jun 2025 11:05:34 +0530
Subject: [PATCH 1/2] kernel: arm64: dts: qcom: enable EEPROM Client Driver

Add dts change to support EEPROM on IQ-9075-evk

Change-Id: Id0d09154a77051cae3e9a839497dda1242a23bdb
Signed-off-by: Linux Build Service Account <lnxbuild@localhost>
Upstream-Status: Pending
---
 arch/arm64/boot/dts/qcom/qcs9075-iq-9075-evk.dts | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/arch/arm64/boot/dts/qcom/qcs9075-iq-9075-evk.dts b/arch/arm64/boot/dts/qcom/qcs9075-iq-9075-evk.dts
index 763208149fe1..e5a0772c551d 100644
--- a/arch/arm64/boot/dts/qcom/qcs9075-iq-9075-evk.dts
+++ b/arch/arm64/boot/dts/qcom/qcs9075-iq-9075-evk.dts
@@ -28,6 +28,7 @@ aliases {
 		spi16 = &spi16;
 		ufshc1 = &ufs_mem_hc;
 		mmc0 = &sdhc_1; /* SDC1 SD card slot */
+		i2c18 = &i2c18;
 	};
 
 	chosen {
@@ -580,6 +581,13 @@ expander3: pca953x@3b {
 		gpio-controller;
 		reg = <0x3b>;
 	};
+
+    eeprom0: 24c256@50 {
+		compatible = "atmel,24c256";
+		reg = <0x50>;
+		pagesize = <64>;
+		i2c_id = <0x12>;
+	};
 };
 
 &i2c19 {
