[Unit]
Description=mount usb_fw unit
Before=local-fs.target

[Mount]
What=/dev/disk/by-partlabel/usb_fw
Where=/var/usbfw
DirectoryMode=0744
Type=ext4

[Install]
WantedBy=local-fs.target
